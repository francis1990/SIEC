<fieldset>
    <div class="">
        <div class="{%- if not form.suministradores.vars.valid %} has-error{% endif %}">
            <div class="pull-right">
                <button title="Adicionar" type="button" class="btn btn-default text-green btn-xs add-form-embedded"
                        data-container="#suministrador-tbody">
                    <i class=" glyphicon glyphicon-plus"></i>
                    Adicionar
                </button>
            </div>
        </div>
        <div class="cont-table-blanco col-xs-12">
            {{ form_errors(form.suministradores) }}

            <table class="table table-striped table-hover table-condensed" id="asegnorma">
                <thead>
                <tr>
                    <th width="45%">Suministrador</th>
                    <th width="50%">Producto</th>
                    <th width="5%"></th>
                </tr>
                </thead>
                <tbody id="suministrador-tbody"
                       data-prototype="{{ form_widget(form.suministradores.vars.prototype)|e }}"
                       data-index="{{ form.suministradores|length * 2}}">
                {% for sumin in form.suministradores %}
                    {{ form_widget(sumin) }}
                {% endfor %}
                </tbody>
            </table>

            {{ form_widget(form.suministradores) }}{# Para que no se muestre nuevamente el texto components #}
        </div>
    </div>
</fieldset>