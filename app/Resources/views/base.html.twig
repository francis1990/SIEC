<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}{% endblock %}SIEC </title>
    {% block css %}
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
        <link href="{{ asset('bundles/tercero/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link rel="stylesheet" href="{{ asset('bundles/tercero/css/font-awesome.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/tercero/css/AdminLTE.css') }}"/>
        <link href="{{ asset('bundles/tercero/css/ionicons.min.css') }}" rel="stylesheet"/>

        {# DATA TABLES#}
        {#<link href="{{ asset('bundles/tercero/css/datatables/dataTables.bootstrap.css') }}" rel="stylesheet"/>#}
        <link href="{{ asset('bundles/tercero/datables/dataTables.bootstrap.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/tercero/datables/jquery.dataTables.min.css') }}" rel="stylesheet"/>
        {# tregrid#}
        <link rel="stylesheet" href="{{ asset('bundles/tercero/jquery-treegrid/css/jquery.treegrid.css') }}">
        {# chosen#}
        <link href="{{ asset('bundles/tercero/js/plugins/chosen/chosen.css') }}" rel="stylesheet" type="text/css"/>

        <link href="{{ asset('bundles/tercero/css/custom.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/tercero/css/proyecto_coopelia.css') }}" rel="stylesheet" type="text/css"/>
        {#Datepicker #}
        <link rel="stylesheet" href="{{ asset('bundles/tercero/css/datepicker/bootstrap-datepicker.css') }}"/>

        <link rel="stylesheet" href="{{ asset('bundles/tercero/simple-line-icons/simple-line-icons.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/tercero/css/font-awesome.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/tercero/css/AdminLTE.css') }}"/>
        <link href="{{ asset('bundles/tercero/css/custom.css') }}" rel="stylesheet" type="text/css"/>
        {#<link rel="stylesheet" href="{{ asset('bundles/tercero/js/jquery-easyui-1.5.3/themes/gray/easyui.css') }}"/>#}
        {#<link rel="stylesheet" href="{{ asset('bundles/tercero/js/jquery-easyui-1.5.3/themes/icon.css') }}"/>#}
        <link href="{{ asset('bundles/tercero/css/proyecto_coopelia_roniel.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/tercero/css/proyecto_coopelia.css') }}" rel="stylesheet" type="text/css"/>

    {% endblock %}
</head>
{% block body %}
<body class="skin-blue" id="{% block id '' %}">
{% endblock %}
{% block flash_message %}
    {% for tipo, mensajes in app.session.flashbag.all() %}
        {% for mensaje in mensajes %}
            <div style="width: 35%;" class="alert alert-{{ tipo }} alert-dismissable flash flash-{{ tipo }}">
                <i class="glyphicon "></i>
                <button type="button" class="close" data-dismiss="alert"
                        aria-hidden="true">×
                </button>
                <p>{{ mensaje }}</p>
            </div>
        {% endfor %}
    {% endfor %}
{% endblock %}
{% block mensajes %}
    <div class="modal fade" id="modal_eliminar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
         aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body ">
                    <h4><i class="glyphicon glyphicon-question-sign"></i> Eliminar</h4>
                    <p>¿Está seguro que desea eliminar el elemento?</p>
                </div>
                <div class="modal-footer">
                    <a id="remove" href="#" class="btn btn-primary" data-dismiss="modal"><i
                                class="glyphicon glyphicon-ok"></i> Aceptar</a>
                    <a id="cancelar" href="#" class="btn btn-default" data-dismiss="modal"><i
                                class="glyphicon glyphicon-remove"></i> Cancelar</a>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="modal_desasociar" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
         aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body ">
                    <h4><i class="glyphicon glyphicon-question-sign"></i> Eliminar</h4>
                    <p>¿Está seguro que desea eliminar el elemento?</p>
                </div>
                <div class="modal-footer">
                    <a id="desasociar" href="#" class="btn btn-primary" data-dismiss="modal"><i
                                class="glyphicon glyphicon-ok"></i> Aceptar</a>
                    <a id="cancelar_des" href="#" class="btn btn-default" data-dismiss="modal"><i
                                class="glyphicon glyphicon-remove"></i> Cancelar</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block cerrar %}
{% endblock %}
{% if is_granted('IS_AUTHENTICATED_FULLY') or is_granted('IS_AUTHENTICATED_REMEMBERED') %}
    <input id="valSessionUser" type="hidden" value="{{ app.user.idsession }}">
{% endif %}
<input id="valSessionActual" type="hidden" value="{{ app.request.session.id }}">
{% block javascripts %}

    <script src="{{ asset('bundles/tercero/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/jquery-ui-1.10.3.min.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/bootstrap.min.js') }}"></script>

    {# DATA TABES SCRIPT #}
    {#<script src="{{ asset('bundles/tercero/js/plugins/datatables/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/plugins/datatables/dataTables.bootstrap.js') }}"></script>#}

    {#<script src="{{ asset('bundles/tercero/datables/jquery-3.3.1.js') }}"></script>#}
    <script src="{{ asset('bundles/tercero/datables/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('bundles/tercero/datables/dataTables.bootstrap.js') }}"></script>

    <script src="{{ asset('bundles/tercero/js/ajaxTable.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/ajaxTreeTable.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/plugins/iCheck/icheck.min.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/AdminLTE/app.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/tercero/jquery-treegrid/js/jquery.treegrid.js') }}"></script>
    {#Datepicker #}
    <script type="text/javascript"
            src="{{ asset('bundles/tercero/js/js_datepicker/bootstrap-datepicker.min.js') }}"></script>
    {# chosen#}
    <script src="{{ asset('bundles/tercero/js/plugins/chosen/chosen.jquery.min.js') }}"></script>
    {# jquery.validate#}
    <script src="{{ asset('bundles/tercero/js/jquery.validate.min.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/plugins/validate/messages_es.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/js_crud/fn_datatables.js') }}"></script>
    <script src="{{ asset('bundles/tercero/js/js_crud/fn_jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/tercero/js/js_crud/fn_modal.js') }}"></script>

    <script type="text/javascript" src="{{ asset('bundles/tercero/js/plugins/bootbox/bootbox.min.js') }}"></script>

    <script type="text/javascript">
        history.forward();
        jQuery(document).ready(function () {
            jQuery('table').addClass('table-hover');
            jQuery('table').addClass('table-condensed');
            jQuery(".chosen-select").chosen({search_contains: true});
        });

        /*Mostrar alerta de salir de session cuando un mismo usuario se autentica en otra PC*/
        var sesionusuario = $('#valSessionUser').val();
        var sesion = $('#valSessionActual').val();

        if (sesionusuario != undefined) {
            if (sesionusuario != sesion) {
                bootbox.alert("La sesión cerrará porque otro usuario accedió al sistema con las mismas credenciales.", function () {
                    lock_screen();
                    document.location = "{{ path('logout') }}";
                });
            }
        }

    </script>

{% endblock %}

{% block more_scripts %}
{% endblock %}
</body>
</html>























